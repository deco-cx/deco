{
  "tasks": {
    "start": "deno run -A --unstable dev.ts",
    "component": "deno eval 'import \"deco/scripts/component.ts\"'",
    "order_imports": "deno eval 'import \"deco/scripts/order_imports.ts\"'",
    "release": "deno run -A ./scripts/release.ts",
    "check": "deno run -A --unstable dev.ts && deno fmt && deno check dev.ts live.ts && deno test -A . && deno bench -A .",
    "cache_clean": "rm deno.lock; deno cache -r live.ts",
    "info_json": "deno info --json live.ts > deps.json",
    "detective": "deno run -A https://deno.land/x/detective/detective.ts deps.json",
    "deps": "deno task cache_clean && deno task info_json && deno task detective",
    "telemetry": "OTEL_EXPORTER_OTLP_METRICS_ENDPOINT=https://api.honeycomb.io/v1/metrics OTEL_EXPORTER_OTLP_TRACES_ENDPOINT=https://api.honeycomb.io/v1/traces OTEL_EXPORTER_OTLP_ENDPOINT=http://api.honeycomb.io OTEL_EXPORTER_OTLP_HEADERS='x-honeycomb-team=rTEKbRcCJ5ciE0blPh0kGC' deno run -A tracer.ts"
  },
  "githooks": {
    "pre-commit": "check"
  },
  "fmt": {
    "files": {
      "exclude": ["static/", "README.md"]
    }
  },
  "importMap": "./import_map.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
